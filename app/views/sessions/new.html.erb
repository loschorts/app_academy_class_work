<h2>Log in:</h2>

<form action="<%= session_url %>" method="post">

<label for="username">Username</label>
<input
  type="text"
  name="user[username]"
  id="username">

<label for="password">Password</label>
<input
  type="password"
  name="user[password]"
  id="password">
<input type="submit">
</form>
<br>
