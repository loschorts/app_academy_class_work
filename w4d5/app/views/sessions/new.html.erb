<h3>Log in!</h3>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <label for="username_id">Username</label>
  <input type="text" name="user[username]" value="<%= @user.username %>" id="username_id">

  <label for="password_id">Password</label>
  <input type="password" name="user[password]" value="<%= @user.password %>" id="password_id">

  <input type="submit" value="Submit">
</form>
