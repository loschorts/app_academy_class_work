<input
type="hidden"
name="authenticity token"
value="<%= form_authenticity_token%>">

<label for="username">Username</label>
<input
  type="text"
  name="user[username]"
  id="username"
  value="<%= @user.username %>">

<label for="password">Password</label>
<input
  type="password"
  name="user[password]"
  id="password">
<input type="submit">
<br>
